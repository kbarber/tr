		<!--[if lt IE 7]><script type="text/javascript" src="https://team.alfresco.com/w/skins/common/IEFixes.js?195"></script>
		<meta http-equiv="imagetoolbar" content="no" /><![endif]-->
</head>
   
<body>
  <h1 id="firstHeading" class="firstHeading">Documentation for TR version: [% doc.version %]</h1>
  
  [% FOREACH path IN doc.paths -%]
    <ul>
      <li><span class="toctext">[% path.key %]</span></li>
      <ul>
      [% FOREACH method IN path.value -%]
        <li><a href="[% path.key %]?method=system.doc&show=[% method %]">[% method %]</a>
        <a href="[% path.key %]?method=system.schema&show=[% method %]">(schema)</a></li>
      [% END %]
      </ul>
    </ul>
  [% END %]
  [% IF doc.poddoc %]
    [% FILTER html_para %]
      <h3>[% doc.method %]</h3>

      [% doc.poddoc FILTER html_line_break %]
    [% END %]
  [% END %]
  [% IF doc.schema %]
    [% FILTER html_para %]
      <h3>Schema for: [% doc.method %]</h3>

      [% doc.schema FILTER html_line_break %]
    [% END %]
  [% END %]
</body>
</html>
